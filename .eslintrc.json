{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": "warn",
    "import/extensions": "off",
    "function-paren-newline": "off",
    "implicit-arrow-linebreak": "off"
  }
}
